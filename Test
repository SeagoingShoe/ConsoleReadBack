open System
open System.Speech.Synthesis

[<EntryPoint>]
let main argv = 
    printfn "Hello world"

    let speechTest = new System.Speech.Synthesis.SpeechSynthesizer()
    speechTest.Speak("hello world")

   // Console.ReadKey(true)
    // printfn "%A" argv

    0 // return an integer exit code
